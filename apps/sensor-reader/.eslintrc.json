{
  "extends": ["../../.eslintrc.json", "oclif", "oclif-typescript"],
  "ignorePatterns": ["!**/*", "bin", "dist"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "node/no-extraneous-import": [
          "error",
          {
            "allowModules": ["@oclif/core"]
          }
        ]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
