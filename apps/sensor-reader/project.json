{
  "root": "apps/sensor-reader",
  "sourceRoot": "apps/sensor-reader/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "shx rm -rf lib && tsc",
          "shx mkdir -p ../../dist/apps/sensor-reader/lib && cp -R lib/* ../../dist/apps/sensor-reader/lib"
        ],
        "parallel": false,
        "cwd": "apps/sensor-reader"
      }
    },
    "pack": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "shx rm -rf ./lib && tsc",
          "shx rm -rf ./dist && rm -rf ./tmp && rm -rf ../../dist/apps/sensor-reader/pack",
          "cp ../../package-lock.json .",
          "oclif manifest",
          "oclif readme",
          "oclif pack:tarballs -t linux-x64 -t linux-arm -t darwin-arm64",
          "shx rm -f .oclif.manifest.json && shx rm -f package-lock.json",
          "shx mkdir -p ../../dist/apps/sensor-reader/pack && mv dist/* ../../dist/apps/sensor-reader/pack"
        ],
        "parallel": false,
        "cwd": "apps/sensor-reader"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/sensor-reader/**/*.ts"
        ]
      }
    }
  },
  "tags": []
}